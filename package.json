  "private": true,
  "scripts": {
    "build": "node build-simple-direct.js",
    "build:shared": "cd shared && npm install && npx tsc -p tsconfig.json",
    "build:server": "cd server && npm install && npm run build",
    "build:client": "cd client && npm install && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "install:all": "npm install --legacy-peer-deps && npm install --legacy-peer-deps --workspaces",
    "start:server": "cd server && npm start",
    "start:client": "cd client && npm start",
    "start:demo": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "dev": "npm run start:demo",
    "demo:ash512": "cd scripts && npx ts-node ash512-performance.ts"
